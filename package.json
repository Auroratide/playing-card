    "test": "npm run build && wtr --node-resolve 'lib/**/*.spec.js'",
    "prepublishOnly": "npm run test"
  },
  "author": {
    "name": "Timothy Foster",
    "url": "https://auroratide.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@open-wc/testing": "^3.0.3",
    "@web/dev-server": "^0.1.28",
    "@web/test-runner": "^0.13.22",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.2"
  }
}
