{
  "name": "@auroratide/playing-card",
  "version": "0.0.1",
  "description": "Web component for a playing card",
  "keywords": [
    "web-component",
    "element",
    "card",
    "poker",
    "casino"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "type": "module",
  "scripts": {
    "start": "wds --node-resolve -p 3000",
